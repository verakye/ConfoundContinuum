# The environment
universe = vanilla
getenv = True

# Resources
request_cpus = 1
request_memory = 5G
request_disk = 20

# Executable
initial_dir = $(pwd)
executable = $(initial_dir)/src/2_phenotype_extraction/run_in_venv.sh
transfer_executable = False

arguments = 1_get_motor_phenotypes.py


# Logs
log = $(initial_dir)/results/2_phenotype_extraction/logs/1_get_motor_phenotypes.log
output = $(initial_dir)/2_phenotype_extraction/logs/1_get_motor_phenotypes.out
error = $(initial_dir)/2_phenotype_extraction/logs/1_get_motor_phenotypes.err

queue